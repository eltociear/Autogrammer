{
  "name": "gbnf",
  "type": "module",
  "version": "0.0.5",
  "repository": {
    "type": "git",
    "url": "https://github.com/thekevinscott/gbnf.git"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs"
    }
  },
  "scripts": {
    "lint": "wireit",
    "test": "vitest",
    "build": "wireit"
  },
  "wireit": {
    "lint": {
      "command": "eslint -c .eslintrc.cjs src --ext .ts"
    },
    "build": {
      "command": "rimraf ./dist && vite build && cp ../../README.md ./README.md",
      "files": [
        "../../README.md",
        "./src/**/*.ts",
        "package.json",
        "vite.config.ts",
        "tsconfig.json"
      ],
      "output": [
        "./dist/**",
        "./README.md"
      ]
    }
  },
  "keywords": [],
  "author": "Kevin Scott",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "vite": "^5.1.4",
    "vite-plugin-dts": "^3.7.3",
    "vitest": "^1.3.1",
    "wireit": "^0.14.4"
  }
}
