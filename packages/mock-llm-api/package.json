{
  "name": "mock-llm-api",
  "version": "0.0.1",
  "type": "module",
  "private": true,
  "main": "./dist/index.js",
  "scripts": {
    "clean": "wireit",
    "build:esm": "wireit",
    "build": "wireit"
  },
  "wireit": {
    "clean": {
      "command": "rimraf ./dist",
      "files": [
        "./dist/**"
      ]
    },
    "build:esm": {
      "command": "tsc"
    },
    "build": {
      "command": "pnpm clean && pnpm build:esm",
      "files": [
        "./src/**/*.ts",
        "!./src/**/*.test.ts",
        "package.json",
        "vite.config.ts",
        "tsconfig.json"
      ],
      "output": [
        "./dist/**"
      ]
    }
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.3",
    "wireit": "^0.14.4"
  },
  "dependencies": {
    "express": "^4.19.2"
  },
  "packageManager": "pnpm@9.1.3"
}
