{
  "name": "@autogrammer/docs",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "wireit",
    "build": "wireit"
  },
  "wireit": {
    "start": {
      "command": "docsanova start --port 8080 -n _nm",
      "dependencies": [
        "../javascript:build",
        "../../docsanova:build"
      ]
    },
    "build": {
      "command": "docsanova build --output build -n _nm",
      "dependencies": [
        "../javascript:build",
        "../../docsanova:build"
      ]
    }
  },
  "dependencies": {
    "@alenaksu/json-viewer": "^2.0.1",
    "@lit/reactive-element": "^2.0.4",
    "@xenova/transformers": "^2.17.1",
    "autogrammer": "workspace:*",
    "contort": "workspace:*",
    "docsanova": "workspace:*",
    "gbnf": "workspace:*",
    "json2gbnf": "workspace:*",
    "lit": "^3.1.3",
    "lit-element": "^4.0.5",
    "lit-html": "^3.1.3",
    "sql2gbnf": "workspace:*"
  },
  "devDependencies": {
    "tslib": "^2.6.2",
    "typescript": "^5.4.5",
    "wireit": "^0.14.4"
  },
  "packageManager": "pnpm@9.1.4"
}
