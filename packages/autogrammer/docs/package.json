{
  "name": "@autogrammer/docs",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "wireit",
    "build": "wireit"
  },
  "wireit": {
    "start": {
      "command": "docsanova start --port 8080",
      "dependencies": [
        "../javascript:build",
        "../../docsanova:build"
      ]
    },
    "build": {
      "command": "docsanova build --output build",
      "dependencies": [
        "../javascript:build",
        "../../docsanova:build"
      ]
    }
  },
  "dependencies": {
    "@mlc-ai/web-llm": "^0.2.35",
    "@vanillawc/wc-codemirror": "^2.1.0",
    "@xenova/transformers": "^2.17.1",
    "autogrammer": "workspace:*",
    "docsanova": "workspace:*",
    "esbuild": "^0.20.2",
    "esbuild-wasm": "^0.20.2",
    "lit": "^3.1.3",
    "playground-elements": "^0.18.1",
    "wireit": "^0.14.4"
  },
  "devDependencies": {
    "tslib": "^2.6.2",
    "typescript": "^5.4.5"
  }
}
