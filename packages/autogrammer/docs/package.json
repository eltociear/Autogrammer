{
  "name": "@autogrammer/docs",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "wireit",
    "build": "wireit"
  },
  "wireit": {
    "start": {
      "command": "docsanova start --port 8080",
      "dependencies": [
        "../javascript:build",
        "../../docsanova:build"
      ]
    },
    "build": {
      "command": "docsanova build --output build",
      "dependencies": [
        "../javascript:build",
        "../../docsanova:build"
      ]
    }
  },
  "dependencies": {
    "@lit/reactive-element": "^2.0.4",
    "@mlc-ai/web-llm": "^0.2.35",
    "@vanillawc/wc-codemirror": "^2.1.0",
    "@xenova/transformers": "^2.17.1",
    "autogrammer": "workspace:*",
    "docsanova": "workspace:*",
    "contort": "workspace:*",
    "json2gbnf": "workspace:*",
    "sql2gbnf": "workspace:*",
    "gbnf": "workspace:*",
    "lit": "^3.1.3",
    "lit-element": "^4.0.5",
    "lit-html": "^3.1.3"
  },
  "devDependencies": {
    "tslib": "^2.6.2",
    "typescript": "^5.4.5",
    "wireit": "^0.14.4"
  }
}
