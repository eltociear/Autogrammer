{
  "name": "contortionist",
  "type": "module",
  "version": "0.0.7",
  "main": "./dist/contortionist.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/thekevinscott/contortionist.git"
  },
  "exports": {
    ".": "./dist/contortionist.js"
  },
  "scripts": {
    "lint": "wireit",
    "test": "vitest",
    "larkgenpython": "lark-js ./src/parsers/lark-parser/grammars/python/python.lark -o ./src/parsers/lark-parser/grammars/python/python-parser.js",
    "lark-gen-json": "lark-js ./src/parsers/lark-parser/grammars/json/json.lark -o ./src/parsers/lark-parser/grammars/json/json-parser.js",
    "genpython": "antlr4ng -Dlanguage=TypeScript -o ./src/parsers/antlr4-parser/grammars/Python3/generated -Xexact-output-dir -visitor ./src/parsers/antlr4-parser/grammars/Python3/Python3Lexer.g4 ./src/parsers/antlr4-parser/grammars/Python3/Python3Parser.g4",
    "genkotlin": "antlr4ng -Dlanguage=TypeScript -o ./src/parsers/antlr4-parser/grammars/Kotlin/generated -Xexact-output-dir -visitor ./src/parsers/antlr4-parser/grammars/Kotlin/KotlinLexer.g4 ./src/parsers/antlr4-parser/grammars/Kotlin/KotlinParser.g4",
    "genjs_backup": "antlr4ng -Dlanguage=TypeScript -o ./src/parsers/antlr4-parser/grammars/JavaScript/generated -Xexact-output-dir -visitor ./src/parsers/antlr4-parser/grammars/JavaScript/JavaScriptLexer.g4 ./src/parsers/antlr4-parser/grammars/JavaScript/JavaScriptParser.g4",
    "genjs": "antlr4 -Dlanguage=JavaScript -o gen -o ./src/parsers/antlr4-parser/grammars/JavaScript/generated -Xexact-output-dir -visitor ./src/parsers/antlr4-parser/grammars/JavaScript/JavaScriptLexer.g4 ./src/parsers/antlr4-parser/grammars/JavaScript/JavaScriptParser.g4",
    "genexpr": "antlr4ng -Dlanguage=TypeScript -o ./src/parsers/antlr4-parser/grammars/Expression/generated -Xexact-output-dir -visitor ./src/parsers/antlr4-parser/grammars/Expression/Expression.g4 ",
    "compile": "antlr4 -Dlanguage=TypeScript Hello.g4 -o generated/",
    "build": "wireit"
  },
  "wireit": {
    "lint": {
      "command": "eslint -c .eslintrc.cjs src --ext .ts"
    },
    "build": {
      "command": "rimraf ./dist && vite build",
      "files": [
        "./src/**/*.json",
        "./src/**/*.ts",
        "./src/**/*.js",
        "package.json",
        "vite.config.ts",
        "tsconfig.json"
      ],
      "output": [
        "./dist/**"
      ]
    }
  },
  "author": "Kevin Scott",
  "license": "MIT",
  "dependencies": {
    "@xenova/transformers": "^2.16.0",
    "gbnf": "^0.0.1"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.6",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "@types/jest": "^29.5.12",
    "rimraf": "^5.0.5",
    "rollup": "^4.12.1",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-esbuild": "^6.1.1",
    "rollup-plugin-string": "^3.0.0",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "vite": "^5.1.5",
    "vite-plugin-dts": "^3.7.3",
    "vitest": "^1.3.1"
  }
}
